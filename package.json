{
  "name": "toiletmap",
  "private": true,
  "version": "2.0.0",
  "description": "",
  "scripts": {
    "build": "REACT_APP_BAKED_BACKEND= react-app-rewired build",
    "dev": "react-app-rewired start --port $PORT",
    "dev:api": "nodemon -r dotenv/config src/api/index.js",
    "lint": "eslint . && stylelint \"src/**/*.css\"",
    "format": "prettier --write \"./**/*.{js,json}\"",
    "test": "exit 0",
    "cy:open": "cypress open --config baseUrl=http://localhost:3000",
    "cy:run": "cypress run --config baseUrl=http://localhost:5000,video=false",
    "test:integration": "start-server-and-test start http://localhost:5000 cy:run",
    "nopostinstall": "yarn configure && cordova prepare",
    "configure": "env-cmd --silent babel-node --presets @babel/preset-react tasks/cordova-config.js",
    "prebuild-phonegap": "yarn prebuild-cordova",
    "build-phonegap": "yarn configure && bestzip phonegap.zip -r www/* package.json config.xml",
    "prebuild-cordova": "yarn build && cpx \"build/**/*\" www/",
    "build-cordova": "yarn configure && cordova requirements && cordova build",
    "predev-cordova": "yarn configure --dev && cordova requirements",
    "dev-cordova": "concurrently -n 'ReactDevServer,AndroidDevInstall' 'yarn dev' 'cordova run android'",
    "dev-cordova-ios": "concurrently -n 'ReactDevServer,AndroidDevInstall' 'yarn dev' 'cordova run ios'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neontribe/gbptm.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neontribe/gbptm/issues"
  },
  "lint-staged": {
    "*.js": [
      "eslint --max-warnings 0",
      "prettier --write"
    ],
    "*.css": "stylelint"
  },
  "dependencies": {
    "@apollo/client": "3.0.0-beta.41",
    "@apollo/link-context": "2.0.0-beta.3",
    "@apollo/link-error": "2.0.0-beta.3",
    "@auth0/cordova": "0.4.3",
    "auth0-js": "9.13.0",
    "apollo-server": "2.11.0",
    "cordova-android": "8.1.0",
    "cordova-ios": "5.1.1",
    "cordova-plugin-customurlscheme": "5.0.1",
    "cordova-plugin-geolocation": "4.0.2",
    "cordova-plugin-safariviewcontroller": "1.6.0",
    "cordova-plugin-whitelist": "1.3.4",
    "core-js": "3.6.4",
    "graphql.macro": "1.4.2",
    "graphql-iso-date": "3.6.1",
    "hasha": "5.2.0",
    "jsonwebtoken": "8.5.1",
    "jwks-rsa": "1.7.0",
    "leaflet": "1.6.0",
    "leaflet-control-geocoder": "1.12.1",
    "leaflet-loading": "0.1.24",
    "leaflet.locatecontrol": "0.71.0",
    "leaflet.markercluster": "1.4.1",
    "lodash": "4.17.15",
    "luxon": "1.22.2",
    "mongoose": "5.9.7",
    "mongoose-paginate": "5.0.3",
    "node-fetch": "2.6.0",
    "normalize.css": "8.0.1",
    "prop-types": "15.7.2",
    "query-string": "5.1.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "5.2.1",
    "react-leaflet": "2.6.3",
    "react-responsive": "8.0.3",
    "react-router-dom": "5.1.2",
    "react-router-ga": "1.2.3",
    "react-router-hash-link": "1.2.2",
    "resize-observer-polyfill": "1.5.1"
  },
  "devDependencies": {
    "@babel/node": "7.8.7",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "bestzip": "2.1.5",
    "concurrently": "5.1.0",
    "cordova": "9.0.0",
    "cpx": "1.5.0",
    "cypress": "4.3.0",
    "cypress-image-snapshot": "3.1.1",
    "dotenv": "8.2.0",
    "env-cmd": "10.1.0",
    "eslint-plugin-cypress": "2.10.3",
    "http-proxy-middleware": "1.0.3",
    "http-server": "0.12.1",
    "husky": "4.2.3",
    "lint-staged": "10.1.1",
    "nodemon": "2.0.2",
    "prettier": "2.0.2",
    "react-app-rewired": "2.1.5",
    "react-scripts": "3.4.1",
    "start-server-and-test": "1.10.11",
    "stylelint": "13.2.1",
    "stylelint-config-standard": "20.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
