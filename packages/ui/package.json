{
  "name": "@toiletmap/ui",
  "version": "3.2.0",
  "private": true,
  "main": "index.js",
  "homepage": "./",
  "devDependencies": {
    "@babel/node": "7.7.4",
    "cordova": "9.0.0",
    "cpx": "1.5.0",
    "cross-env": "6.0.3",
    "cypress": "3.1.4",
    "cypress-image-snapshot": "3.0.0",
    "env-cmd": "10.0.1",
    "eslint-plugin-cypress": "2.2.0",
    "http-proxy-middleware": "0.19.1",
    "http-server": "0.11.1",
    "react-app-rewired": "2.1.3",
    "react-scripts": "3.2.0",
    "serve": "10.1.1",
    "start-server-and-test": "1.7.11",
    "yargs": "15.0.2"
  },
  "dependencies": {
    "@toiletmap/api-client": "3.2.0",
    "@toiletmap/leaflet-control-geocoder": "3.2.0",
    "@toiletmap/opening-hours": "3.2.0",
    "auth0-js": "9.10.0",
    "concurrently": "5.0.0",
    "cordova-android": "^8.1.0",
    "cordova-plugin-geolocation": "^4.0.2",
    "cordova-plugin-whitelist": "^1.3.4",
    "core-js": "2.6.3",
    "js-cookie": "2.2.1",
    "leaflet": "1.5.1",
    "leaflet-loading": "0.1.24",
    "leaflet.locatecontrol": "0.67.0",
    "leaflet.markercluster": "1.4.1",
    "lodash": "4.17.15",
    "normalize.css": "8.0.1",
    "prop-types": "15.6.2",
    "react": "16.7.0",
    "react-addons-update": "15.6.2",
    "react-dom": "16.7.0",
    "react-helmet": "5.2.1",
    "react-leaflet": "2.4.0",
    "react-redux": "6.0.0",
    "react-responsive": "6.1.1",
    "react-router-dom": "4.3.1",
    "react-router-ga": "1.1.0",
    "react-router-hash-link": "1.2.2",
    "redux": "4.0.4",
    "redux-saga": "1.0.0",
    "resize-observer-polyfill": "1.5.1"
  },
  "scripts": {
    "dev": "cross-env SKIP_PREFLIGHT_CHECK=true react-app-rewired start",
    "start": "http-server ./build -p 5000 -a localhost --proxy https://gbptm-stage.herokuapp.com",
    "build": "cross-env SKIP_PREFLIGHT_CHECK=true react-app-rewired build",
    "cy:open": "cypress open --config baseUrl=http://localhost:3000",
    "cy:run": "cypress run --config baseUrl=http://localhost:5000,video=false",
    "test:integration": "start-server-and-test start http://localhost:5000 cy:run",
    "configure": "env-cmd babel-node --presets @babel/preset-react tasks/cordova-config.js",
    "prebuild-cordova": "cordova requirements && cross-env REACT_APP_CORDOVA_BUILD=true yarn build && cpx \"build/**/*\" www/",
    "build-cordova": "cordova build",
    "predev-cordova": "yarn configure --dev && cordova requirements",
    "dev-cordova": "concurrently -n 'ReactDevServer,AndroidDevInstall' 'cross-env REACT_APP_CORDOVA_BUILD=true yarn dev' 'cordova run android'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-geolocation": {}
    },
    "platforms": [
      "android"
    ]
  }
}
